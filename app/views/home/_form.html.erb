
<div class="clearfix">
   <%= f.label :_type, t("activerecord.attributes.home._type", :default => " type"), :class => :label %>
  <div class="input">
    <%= f.text_field :_type, :class => 'text_field' %>
  </div>
</div>

<div class="clearfix">
   <%= f.label :_id, t("activerecord.attributes.home._id", :default => ""), :class => :label %>
  <div class="input">
    <%= f.text_field :_id, :class => 'text_field' %>
  </div>
</div>
<div class="actions">
  <button class="btn primary" type="submit">Save</button> or
	<%= link_to "Cancel", home_path %>
</div>
